{
  "name": "@halvaradop/repo-core",
  "version": "1.0.0",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "tsc -w",
    "dev:tsup": "tsup -w",
    "dev:vite": "vite",
    "build": "tsc",
    "build:tsup": "tsup",
    "build:vite": "vite build",
    "build:all": "pnpm build:tsup && pnpm build:vite",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@halvaradop/auth-init": "^0.0.3",
    "@halvaradop/repo-config": "workspace:*"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/tsup/index.d.ts",
      "import": "./dist/tsup/index.js",
      "require": "./dist/tsup/index.cjs"
    },
    "./tsup": {
      "types": "./dist/tsup/index.d.ts",
      "import": "./dist/tsup/index.js",
      "require": "./dist/tsup/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.js",
      "require": "./dist/vite/index.cjs"
    }
  }
}
