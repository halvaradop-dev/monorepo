{
  "display": "Node 20",
  "_version": "20.1.0",
  "compilerOptions": {
    "lib": ["DOM", "ES2022"],
    "target": "ES2022",
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "module": "Node16",
    "moduleDetection": "force",
    "moduleResolution": "Node16",
    "declaration": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "baseUrl": ".",
  }
}
