{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:vite": {
      "cache": true,
      "dependsOn": ["^build:vite"],
      "inputs": ["src"],
      "outputs": ["dist"]
    },
    "build:tsup": {
      "cache": true,
      "dependsOn": ["^build:tsup"],
      "inputs": ["src"],
      "outputs": ["dist"]
    },
    "build:all": {
      "cache": true,
      "dependsOn": ["^build:all"],
      "inputs": ["src"],
      "outputs": ["dist"]
    },
    "test": {
      "cache": false,
      "inputs": ["test"]
    },
    "test:watch": {
      "cache": false,
      "persistent": true,
      "inputs": ["test"]
    },
    "dev": {
      "cache": false
    },
    "dev:tsup": {
      "cache": false
    },
    "dev:vite": {
      "cache": false
    }
  },
  "cacheDir": ".turbo/cache"
}
