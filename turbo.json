{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["tsconfig.json"],
  "ui": "tui",
  "cacheDir": ".turbo/cache",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**/*"],
      "inputs": ["packages/**/src/**/*.ts"],
      "cache": true
    }
  }
}
